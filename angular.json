{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "realestateProjectUsingAngular": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "i18n": {
        "sourceLocale": "en",
        "locales": {
          "fr": "src/assets/i18n/fr.json",
          "am": "src/assets/i18n/am.json"
        }
      },
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "localize": true,
            "outputPath": "dist/realestateProjectUsingAngular",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "localize": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "fr": {
              "localize": ["fr"],
              "outputPath": "dist/realestateProjectUsingAngular-fr/"
            },
            "am": {
              "localize": ["am"],
              "outputPath": "dist/realestateProjectUsingAngular-am/"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "realestateProjectUsingAngular:build:production"
            },
            "fr": {
              "buildTarget": "realestateProjectUsingAngular:build:fr"
            },
            "am": {
              "buildTarget": "realestateProjectUsingAngular:build:am"
            },
            "development": {
              "buildTarget": "realestateProjectUsingAngular:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "realestateProjectUsingAngular:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "tsconfig.spec.json",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.css"],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
